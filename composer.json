{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Application Template",
    "keywords": ["yii", "framework", "advanced", "application template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "dev-master",
        "yiisoft/yii2-redis": "*",
        "yiisoft/yii2-sphinx": "*",
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-faker": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "*",

        "cboden/Ratchet": "*",
        "michelf/php-markdown": "*",

        "kakserpom/phpdaemon": "*",

        "nizsheanez/yii2-asset-converter": "1.*",
        "nizsheanez/yii2-websocket-application": "*",

        "danielstjules/stringy": "*",

        "twbs/bootstrap": "3.1.*",
        "fabpot/goutte": "*",
        "codeception/codeception": "*",

        "evernote/evernote": "1.*"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\InstallHandler::setPermissions"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nizsheanez/phpdaemon.git"
        }
    ],
    "extra": {

        "yii-install-writable": [
            "backend/runtime",
            "backend/web/assets",

            "console/runtime",
            "console/migrations",

            "frontend/runtime",
            "frontend/web/assets"
        ]
    }
}
